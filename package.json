{
  "name": "web-favorites",
  "version": "1.0.0",
  "description": "A browser extension for better bookmark management",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "zip": "bestzip extension.zip dist/*",
    "package": "npm run build && npm run zip",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/chrome": "^0.0.254",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/testing-library__react": "^10.0.1",
    "autoprefixer": "^10.4.16",
    "bestzip": "^2.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "rimraf": "^5.0.10",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
